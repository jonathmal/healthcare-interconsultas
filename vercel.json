// healthcare-interconsultas/vercel.json
{
    "version": 2,
    "framework": "nextjs",
    "outputDirectory": ".next",
    "buildCommand": "cd ../../ && npm install && npm run build",
    "installCommand": "npm install",
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "${NEXT_PUBLIC_API_URL}/api/:path*"
      }
    ]
  }