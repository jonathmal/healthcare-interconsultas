{
    "version": 2,
    "builds": [
      {
        "src": "apps/web/package.json",
        "use": "@vercel/next"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "apps/web/$1"
      }
    ],
    "buildCommand": "cd apps/web && npm run build",
    "installCommand": "npm install",
    "outputDirectory": "apps/web/.next"
  }